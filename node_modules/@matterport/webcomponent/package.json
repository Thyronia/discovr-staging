{
  "name": "@matterport/webcomponent",
  "files": [
    "bin/matterport-assets.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "built-bundle",
    "types.d.ts"
  ],
  "bin": {
    "matterport-assets": "bin/matterport-assets.cjs"
  },
  "main": "dist/matterport-viewer.umd.js",
  "module": "dist/matterport-viewer.esm.js",
  "exports": {
    ".": {
      "import": "./dist/matterport-viewer.esm.js",
      "require": "./dist/matterport-viewer.umd.js"
    }
  },
  "types": "./types.d.ts",
  "version": "0.1.26",
  "dependencies": {
    "lit": "^2.4.1"
  },
  "peerDependencies": {
    "three": "^0.146.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "build": "yarn clean && yarn build:bundle:copy && yarn build:component",
    "build:bundle:copy": "yarn workspace @matterport/bundle build && yarn workspace @matterport/bundle dist:copy $(pwd)/built-bundle",
    "build:bundle:link": "yarn workspace @matterport/bundle build && yarn workspace @matterport/webcomponent bundle:link",
    "build:component": "webpack --config ./webpack.prod.config.cjs",
    "build:quick": "yarn build:component",
    "bundle:link": "yarn workspace @matterport/bundle dist:link $(pwd)/built-bundle",
    "clean": "rm -rf ./dist && rm -rf ./built-bundle",
    "dev": "yarn dev:bundle:link-if-needed && webpack serve --config ./webpack.dev.config.cjs",
    "dev:bundle": "yarn workspace @matterport/bundle dev",
    "dev:bundle:link-if-needed": "if [ ! -d \"./built-bundle\" ]; then yarn build:bundle:link; fi;",
    "dev:component": "webpack --config ./webpack.dev.config.cjs",
    "dev:memlab": "memlab run --scenario ./tools/memlab-test.js",
    "dev:vite": "yarn dev:bundle:link-if-needed && yarn dev:component && yarn workspace @matterport/webcomponent-demo-vite dev",
    "dev:webpack": "yarn dev:bundle:link-if-needed && yarn dev:component && yarn workspace @matterport/webcomponent-demo-webpack dev",
    "prepare-public": "yarn workspace @matterport/bundle dist:copy $(pwd)/built-bundle && yarn replace-mp && cp ./built-bundle/LICENSE.md ./LICENSE.md",
    "replace-mp": "replace-in-file /@matterport/g @matterport *.*,**/*.* --isRegex"
  },
  "devDependencies": {
    "@matterport/webpack": "^1.0.0",
    "lit": "^2.4.1",
    "memlab": "^1.1.38",
    "node-html-parser": "^6.1.4",
    "replace-in-file": "^6.3.5",
    "three": "^0.146.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.6.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "matterport": {
    "mp_webgl": "23.5.1_webgl-55-g400f035a68"
  }
}
